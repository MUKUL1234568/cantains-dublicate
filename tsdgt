 unordered_map<char,int>m;
		    string s;
		    for(int i=0;i<A.size();i++)
		    {
		        m[A[i]]++;
		        int f=0;
		        for(int j=0;j<=i;j++)
		        {
		            if(m[A[j]]==1)
		            {
		            s.push_back(A[i]);
		            f=1;
		            break;
		            }
		        }
		        if(f==0)
		        s.push_back('#');
		    }
		    return s;
		}
